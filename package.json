{
  "name": "magicpython",
  "version": "1.2.0",
  "engines": {
    "atom": "*",
    "node": "*",
    "vscode": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MagicStack/MagicPython.git"
  },
  "bugs": {
    "url": "https://github.com/MagicStack/MagicPython/issues"
  },
  "description": "Syntax highlighter for cutting edge Python.",
  "license": "MIT",
  "publisher": "QuentiumYT",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "python",
    "highlighter",
    "syntax",
    "python3"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#2c2c2c",
    "theme": "dark"
  },
  "contributes": {
    "//": [
      "This section is for VSCode.",
      "NOTE: remember to update grammar/src/MagicPython.syntax.yaml, too."
    ],
    "languages": [
      {
        "id": "python",
        "aliases": [
          "MagicPython",
          "python"
        ],
        "extensions": [
          ".py",
          ".py3",
          ".rpy",
          ".pyw",
          ".cpy",
          ".pyi",
          ".SConstruct",
          ".Sconstruct",
          ".sconstruct",
          ".SConscript",
          ".gyp",
          ".gypi",
          ".wsgi",
          ".kv",
          "Snakefile",
          ".smk",
          ".tac"
        ]
      }
    ],
    "grammars": [
      {
        "language": "python",
        "scopeName": "source.python",
        "path": "./grammars/MagicPython.tmLanguage"
      }
    ]
  },
  "devDependencies": {
    "oniguruma": "^7.2.3",
    "syntaxdev": "^0.1.3"
  }
}
